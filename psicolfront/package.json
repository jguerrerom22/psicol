{
  "name": "tareas-planetaip",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "axios": "~0.19.0",
    "bootstrap": "~4.3.1",
    "bootstrap-vue": "2.0.4",
    "sweet-modal-vue": "~2.0.0",
    "vue": "~2.6.6",
    "vue-meta": "~2.3.1",
    "vue-notification": "~1.3.7",
    "vue-router": "~3.1.3",
    "vue-session": "^1.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.3.3",
    "node-sass": "~4.12.0",
    "rename-webpack-plugin": "^2.0.0",
    "sass-loader": "^8.0.0",
    "suppress-chunks-webpack-plugin": "^1.0.5",
    "vue-template-compiler": "~2.6.6",
    "@vue/cli-plugin-e2e-nightwatch": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chromedriver": "77",
    "geckodriver": "^1.16.2"
  },
  "resolutions": {
    "sass-loader/node-sass": "~4.12.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "/dist/",
    "/*.js",
    "/tests/unit/coverage/",
    "src/vendor/libs/**/*.js"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 versions",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
